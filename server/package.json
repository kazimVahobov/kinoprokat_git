{
    "name": "rkm",
    "version": "1.0.1",
    "description": "fullstack api with  angular and express ",
    "main": "app.js",
    "engines": {
        "node": "10.7.0",
        "npm": "6.1.0"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "node app.js",
        "server": "nodemon app.js",
        "client-install": "npm install --prefix client",
        "client": "npm run start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "heroku-postbuild": "npm run client-install && npm run build --prod --prefix client"
    },
    "keywords": [
        "express",
        "angular6",
        "mongoDB"
    ],
    "author": "Faxa",
    "license": "ISC",
    "dependencies": {
        "app-root-path": "2.1.0",
        "bcrypt-nodejs": "0.0.3",
        "bcryptjs": "2.4.3",
        "body-parser": "1.18.2",
        "concurrently": "3.6.1",
        "cors": "2.8.4",
        "express": "4.16.3",
        "express-fileupload": "1.0.0",
        "jsonwebtoken": "8.2.1",
        "moment": "2.22.1",
        "mongoose": "5.7.5",
        "morgan": "1.9.1",
        "multer": "1.3.0",
        "passport": "0.4.0",
        "passport-jwt": "4.0.0",
        "winston": "2.4.3"
    },
    "devDependencies": {
        "nodemon": "^1.18.9"
    }
}
